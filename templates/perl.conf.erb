<LoadPlugin "perl">
    Globals true
</LoadPlugin>

<Plugin "perl">
    BaseName "Collectd::Plugins"
    IncludeDir "<%= @includedir %>"

    LoadPlugin "<%= @module %>"
    <Module "<%= @module %>">
<% @config.sort.each do |key,value| -%>
		<%= key -%> "<%= value -%>"
<% end -%>
    </Module>
</Plugin>
